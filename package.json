{
  "name": "skjobs",
  "version": "0.1.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "11.15.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "yarn build",
    "start": "node server.js",
    "deploy": "git push heroku master"
  },
  "dependencies": {
    "axios": "0.19.0",
    "express": "4.17.1",
    "serve-static": "1.14.1",
    "vue": "2.6.10",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.11.0",
    "@vue/cli-plugin-eslint": "3.11.0",
    "@vue/cli-service": "3.11.0",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
